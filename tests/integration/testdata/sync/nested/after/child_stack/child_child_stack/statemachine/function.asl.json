{
    "Comment": "A Hello World example of the Amazon States Language using Pass states",
    "StartAt": "Hello",
    "States": {
        "Hello": {
            "Type": "Pass",
            "Result": "Hello",
            "Next": "World"
        },
        "World": {
            "Type": "Pass",
            "Result": "World 2",
            "End": true
        }
    }
}